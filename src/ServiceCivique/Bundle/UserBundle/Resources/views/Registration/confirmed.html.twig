{% extends "FOSUserBundle::layout.html.twig" %}

{% block tags %}
{% if is_granted('ROLE_JEUNE') %}
    <script type="text/javascript">
        var axel = Math.random() + "";
        var a = axel * 10000000000000;
        document.write('<iframe src="http://fls.doubleclick.net/activityi;src=3452220;type=servi660;cat=2012-431;ord=1;num=' + a + '?" width="1" height="1" frameborder="0" style="display:none"></iframe>');
    </script>
    <noscript>
        <iframe src="http://fls.doubleclick.net/activityi;src=3452220;type=servi660;cat=2012-431;ord=1;num=1?" width="1" height="1" frameborder="0" style="display:none"></iframe>
    </noscript>
{% endif %}
{% endblock %}

{% block main_content_header %}Cr√©ation de compte{% endblock %}

{% block main_content_body %}
    <div class="alert alert-success"><p>{{ 'registration.confirmed'|trans({'%firstname%': user.firstname}, 'FOSUserBundle') }}</p>
    {% if app.session is not empty %}
        {% set targetUrl = app.session.get('_security.' ~ app.security.token.providerKey ~ '.target_path') %}
        {% if targetUrl is not empty %}<p><a href="{{ targetUrl }}">{{ 'registration.back'|trans({}, 'FOSUserBundle') }}</a></p></div>{% endif %}
    {% endif %}
{% endblock main_content_body %}
